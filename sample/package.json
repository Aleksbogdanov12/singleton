{
  "name": "dev-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "collect-main": "collect-source-bundle --source-dir `pwd`/src/app/source --product SingletonSample --component default --host http://localhost:8091 --version 1.0.0",
    "collect-mixin": "collect-mixin-source --source-dir `pwd`/src/app/modules/l10n --product SingletonSample --component default --host https://localhost:8091 --version 1.0.0",
    "collect-contact": "collect-source-bundle --source-dir `pwd`/src/app/modules/lazy-module --product SingletonContact --component default --host http://localhost:8091 --version 1.0.0",
    "collect-header": "collect-source-bundle --source-dir `pwd`/src/app/modules/vip-header --product SingletonHeader --component default --host http://localhost:8091 --version 1.0.0",
    "load-translation": "load-translation --directory `pwd`/src/assets/translations --product SingletonSample --component default --host http://localhost:8091 --version 1.0.0 --languages zh-Hans"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.0.0",
    "@angular/common": "~7.0.0",
    "@angular/compiler": "~7.0.0",
    "@angular/core": "~7.0.0",
    "@angular/forms": "~7.0.0",
    "@angular/http": "~7.0.0",
    "@angular/platform-browser": "~7.0.0",
    "@angular/platform-browser-dynamic": "~7.0.0",
    "@angular/router": "~7.0.0",
    "@clr/angular": "^1.1.3",
    "@clr/icons": "^1.1.3",
    "@clr/ui": "^1.1.3",
    "@singleton-i18n/angular-client": "file:../singleton-i18n-angular-client-0.1.0.tgz",
    "@webcomponents/custom-elements": "^1.0.0",
    "core-js": "^2.5.4",
    "decimal.js-light": "^2.5.0",
    "rxjs": "~6.3.3",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.6",
    "@angular/compiler-cli": "~7.0.0",
    "@angular/language-service": "~7.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.6",
    "superagent": "^3.8.3"
  }
